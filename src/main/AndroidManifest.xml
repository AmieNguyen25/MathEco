<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.trannguyen.android.matheco">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MathEco"
        tools:targetApi="31">
        <activity
            android:name=".TestEasyActivity"
            android:exported="false" />
        <activity
            android:name=".TestHardActivity"
            android:exported="false" />
        <activity
            android:name=".TestMediumActivity"
            android:exported="false" />
        <activity
            android:name=".Result"
            android:exported="false" />
        <activity
            android:name=".CatScreenHard"
            android:exported="false" />
        <activity
            android:name=".CatScreenMedium"
            android:exported="false" />
        <activity
            android:name=".CatScreen"
            android:exported="false" />
        <activity
            android:name=".Levelscreen"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>